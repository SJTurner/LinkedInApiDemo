@model LinkedInApiDemo.Models.ProfileModel
@{
    ViewBag.Title = "Profile";
}
<h2>Education</h2>
@Model.FirstName @Model.LastName
<hr/>
@{
    foreach (var school in @Model.Educations.Schools)
    {
        <div>
            <div>
                <b>School:</b> @school.SchoolName
            </div>
            <div>
                <b>Major:</b> @school.FieldOfStudy
            </div>
            <div>
                <b>Degree:</b> @school.Degree
            </div>
        </div>
        <hr/>
    }
}

